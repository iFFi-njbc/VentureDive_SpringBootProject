<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=
       "http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.2.xsd"
       xmlns:context="http://www.springframework.org/schema/context">
 
 	<context:annotation-config/>
 
 <bean class="com.SpringFramework.spring1.autowiredAnnotation.Address" name="address1">
 	<property name="street" value="Madina"/>
 	<property name="city" value="Rizwan"/>
 </bean>
 
  <bean class="com.SpringFramework.spring1.autowiredAnnotation.Address" name="address2">
 	<property name="street" value="STREET"/>
 	<property name="city" value="LAHORE"/>
 </bean>
 
 <bean class="com.SpringFramework.spring1.autowiredAnnotation.Employee" name="Employee" p:Name="Ifrah"/>
 
 
 </beans>